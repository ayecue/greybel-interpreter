test = {}
test.foo = function(a, b, c)
    return "test"
end function

test.ama = function(a, b, c)
    return a * b + c
end function

test2 = new test
test.moo = function(a, b, c)
    return super.foo
end function

test3 = new test2
test3.bar = function(a, b, c)
    print(super.moo)
    print(super.ama(3, 3, 1))
end function

test3.bar